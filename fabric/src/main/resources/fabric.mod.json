{
  "schemaVersion": 1,
  "id": "too_many_entities",
  "version": "${version}",
  "name": "Too Many Entities",
  "description": "This mod allows you to hide extra entities in order to reduce lag, especially with mob farms. The mobs still exist in the world, they are just not visible.",
  "authors": [
    "Discusser"
  ],
  "contact": {
    "sources": "https://github.com/Discusser/TooManyEntities"
  },
  "license": "MIT",
  "icon": "assets/too_many_entities/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.discusser.toomanyentities.fabric.TooManyEntitiesClientFabric"
    ],
    "modmenu": [
      "io.github.discusser.toomanyentities.fabric.TooManyEntitiesModMenuApiImpl"
    ]
  },
  "mixins": [
    "too_many_entities.mixins.json",
    "too_many_entities-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
    "fabric-api": "*",
    "cloth-config": ">=11.1.118"
  },
  "recommends": {
    "entityculling": ">=1.6.6",
    "modmenu": ">=7.2.2"
  },
  "custom": {
    "modmenu": {
      "update_checker": true
    }
  }
}
